{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/react-in-jsx-scope":"off",
        "react/prop-types":"off",
        "quotes": [2, "single", { "avoidEscape": true }],
        "indent": [2, 2],
        "eqeqeq": "error",
        "no-unused-vars": "error",
        "no-undef":"off",
        "object-curly-spacing": [
            "error",
            "always"
          ]
    }
}
